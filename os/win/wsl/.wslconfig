# C:\Users\<YourUsername>\.wslconfig
[wsl2]
memory=40GB
processors=20
swap=8GB
networkingMode=mirrored
dnsTunneling=true
firewall=true
autoProxy=true
nestedVirtualization=false
localhostForwarding=true
vmIdleTimeout=-1
guiApplications=false

[experimental]
autoMemoryReclaim=gradual
sparseVhd=true
hostAddressLoopback=true
